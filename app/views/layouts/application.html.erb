<!DOCTYPE html>
<html>
<% if alert %>
   <div class="alert alert-danger">
     <%= alert %>
   </div>
 <% end %>
  <head>

    <p class="notice"><%= notice %></p>
     <p class="alert"><%= alert %></p>
    <title>Blog</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
